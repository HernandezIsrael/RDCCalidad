@model List<Model.spSelHistoricoPagos_Result>
    @{
        ViewBag.Title = "LibHNurc";
        Layout = "~/Views/Shared/_Layout.cshtml";
    }
<div class="card" style="background-color:whitesmoke;">
    <div class="card-content">
        <div class="row">
            <div class="col-sm-12">
                <div class="card">
                    <div class="card-header card-header-icon" data-background-color="green">
                        <i class="material-icons">&#xE894;</i>
                    </div>
                    <div class="card-content">
                        <h4 class="card-title">Proyectos</h4>
                        <div class="table-responsive table-sales">
                            <table id="datatables" class="table table-hover">
                                <thead class="text-danger">
                                  
                                        <tr>
                                            <th>#</th>
                                            @foreach (var item in Model.GroupBy(x => new { x.Estatus_Pago, x.F_Alta }))
                                            {
                                               
                                                <th>@item.Key.F_Alta.Value.ToShortDateString()</th>
                                            }
                                        </tr>
                                   
                                </thead>
                                <tbody>
                                    @foreach (var item in Model.GroupBy(x => new { x.Estatus_Pago, x.F_Alta }))
                                    {
                                        <tr>
                                            <td>@item.Key.Estatus_Pago</td>
                                          
                                                
                                                <td>@item.Key.F_Alta.Value.ToShortDateString().Count()</td>

                                           
                                        </tr>
                                    }
                                </tbody>
                             
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="row">

        </div>
    </div>
</div>

